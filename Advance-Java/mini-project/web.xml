<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>signUP</servlet-name>
        <servlet-class>signUP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>LogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>showResult</servlet-name>
        <servlet-class>showResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewResult</servlet-name>
        <servlet-class>viewResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testSolution</servlet-name>
        <servlet-class>testSolution</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProfile</servlet-name>
        <servlet-class>AddProfile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signUP</servlet-name>
        <url-pattern>/signUP</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>showResult</servlet-name>
        <url-pattern>/showResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewResult</servlet-name>
        <url-pattern>/viewResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testSolution</servlet-name>
        <url-pattern>/testSolution</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProfile</servlet-name>
        <url-pattern>/AddProfile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
